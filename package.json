{
  "name": "db-info",
  "description": "Node.js relational database information utility",
  "keywords": ["database", "db", "sqlite", "mysql", "PostgreSQL", "oracle", "db-oracle"],
  "version": "0.0.3",
  "engines": { "node" : ">=0.10.0" },
  "author": "Joe Ferner <joe.ferner@nearinfinity.com>",
  "maintainers": [
    { "name": "Joe Ferner", "email": "joe.ferner@nearinfinity.com" },
    { "name": "Jeff Kunkle", "email": "jeff.kunkle@nearinfinity.com" }
  ],
  "bugs": { "url": "https://github.com/nearinfinity/node-db-info/issues" },
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/nearinfinity/node-db-info.git" },
  "dependencies": {
  },
  "devDependencies": {
    "tedious": "^1.13.2",
    "sqlite3": "^3.1.1",
    "pg": "^4.4.4",
    "pg-query-stream": "^1.0.0",
    "mysql": "^2.10.2"
  },
  "optionalDependencies": {
    "oracledb": "^1.5.0"
  },
  "scripts": {
    "test": "istanbul cover _mocha -- --recursive ./tests && cat ./coverage/lcov.info | codecov",
    "appveyor": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive ./tests && cat ./coverage/lcov.info | codecov",
    "simpletest": "mocha --recursive ./tests",
    "localtest": "export GIT_BRANCH=`git rev-parse --abbrev-ref HEAD`; export GIT_COMMIT=`git rev-parse HEAD`; istanbul cover -x 'lib/drivers/sqlite3_parser.js' _mocha -- -R spec --recursive ./tests && cat ./coverage/lcov.info | codecov"
  },
  "devDependencies": {
    "mocha": "*",
    "istanbul": "*"
  }
}
